<div class="h-[40vh]"></div>
<section id="logos" class="w-[80vw] mx-auto flex flex-col items-center">
    <div class="w-[50vw] mx-auto">
        <div class="fittext text-center">Click on a logo to download a SVG</div>
    </div>
    
    <button onclick='downloadSVG(this.id)' id="themedLight" class="border-0 bg-lghtgrey1 w-full mx-auto flex justify-center">
        {% svg 'logo_themed_light', 'id="themed--light" class="m-16 max-w-md"' %}
    </button>
    
    <button onclick='downloadSVG(this.id)' id="themedDark" class="border-0 bg-darktone w-full mx-auto flex justify-center">
        {% svg 'logo_themed_dark', 'id="themed--dark" class="m-16 max-w-md"' %}
    </button>

    <button onclick='downloadSVG(this.id)' id="BW-BG-blanc" class="border-0 w-full bg-whitegrey1 mx-auto flex justify-center">
        {% svg 'logo_white', 'id="white" class="m-16 max-w-md"' %}
    </button>
    <button onclick='downloadSVG(this.id)' id="BW-BG-noir" class="w-full border-0 bg-darkgrey1 mx-auto flex justify-center">
        {% svg 'logo_black', 'id="black" class="m-16 max-w-md"' %}
    </button>
</section>