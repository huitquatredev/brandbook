        {% svg 'logo', 'id="blanc" class="m-16 max-w-md"' %}
    </div>
    <div class="w-full bg-[#000] mx-auto flex justify-center">
        {% svg 'logo', 'id="noir" class="m-16 max-w-md"' %}
